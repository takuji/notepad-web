<div id="templates">
  <script type="text/template" class="note-list-item">
    <div class="title">{{title}}</div>
    <div class="created_at">
      <span>{{updated_at}}</span>
      <ul class="note-index-actions">
        <li class="edit">
          <i class="icon-edit"></i>
        </li>
        <li class="delete">
          <i class="icon-trash"></i>
        </li>
      </ul>
    </div>
  </script>

  <script type="text/template" class="note-index-actions-template">
    <ul class="note-index-actions">
      <li class="edit">
        <i class="icon-edit"></i>
      </li>
      <li class="delete">
        <i class="icon-trash"></i>
      </li>
    </ul>
  </script>

  <script type="text/template" class="note-list-item-template">
    <div>
      <span>{{title}}</span>
    </div>
    <div>
      <span class="created_at">{{created_at}}</span>
    </div>
  </script>

  <script type="text/template" class="note-info">
    <span class="key">Created: </span>
    <span class="value">{{created_at}}</span>
    <span class="key">Updated: </span>
    <span class="value">{{updated_at}}</span>
  </script>
</div>
