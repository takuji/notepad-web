<div id="templates">
  <div class="note-index">
    <li data-id="{{id}}">
      <a href="{{link}}">{{title}}</a>
    </li>
  </div>

  <script type="text/template" class="note-index-actions-template">
    <ul class="note-index-actions">
      <li class="delete">
        <i class="icon-remove"></i>
        Delete
      </li>
    </ul>
  </script>

  <script type="text/template" class="note-list-item-template">
    <div>
      <span>{{title}}</span>
    </div>
    <div>
      <span class="created_at">{{created_at}}</span>
    </div>
  </script>
</div>
